{
  "manifest_version": 3,
  "name": "Extrator de Leads - Google Maps byRick",
  "version": "1.0.0",
  "description": "Extrai leads do Google Maps e envia para seu webhook.",
  "permissions": ["tabs", "storage", "scripting", "webNavigation", "activeTab"],
  "host_permissions": ["*://*.google.com/*", "*://*.google.com.br/*"],
  "background": { "service_worker": "background.js" },
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
